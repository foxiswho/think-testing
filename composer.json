{
    "name": "foxiswho/think-testing",
    "license": "Apache-2.0",
    "type": "think-testing",
    "authors": [
        {
            "name": "foxiswho",
            "email": "foxiswho@gmail.com"
        }
    ],
    "require": {
        "topthink/think-installer": "^2.0",
        "topthink/framework": "5.1.*",
        "topthink/think-helper": "~1.0",
        "symfony/dom-crawler": "4.*",
        "phpunit/phpunit": "7.*"
    },
    "config": {
        "bin-dir":"bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "autoload": {
        "psr-4": {
            "think\\testing\\": "src"
        },
        "files": [
            "src/config.php"
        ]
    }
}
